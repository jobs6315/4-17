int main()
{
	int i = 0;
	int k = 0;
	scanf_s("%d", &k);
	for (i = 0;i<=k;i++)
	{
		int j = 0;
		for (j = 0;j < i;j++)
		{
			int ret = i * j;
			printf("%d*%d=%d ", i, j, ret);
		}
		printf("\n");
	}
	return 0;
}
